{
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "importOrder": [
    "^react",
    "^next",
    "^@(.*)$",
    "^i18next(.*)",
    "^(?!(src|../|./))(.*)",
    "^src(.*)$",
    "^(.*)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
